{
  "name": "project-mongo-db",
  "version": "1.0.2-beta",
  "description": "Project Mongo Rest Api client with full offline supprt",
  "main": "src/index.js",
  "react-native": "src/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kriit24/project-mongo-db.git"
  },
  "keywords": [
    "react-native mongo client"
  ],
  "author": "Kuno Vaik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kriit24/project-mongo-db/issues"
  },
  "homepage": "https://github.com/kriit24/project-mongo-db/issues#readme",
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.2",
    "@types/react-native": "0.70.0",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-native": "^0.72.4",
    "react-native-builder-bob": "^0.20.0",
    "react-test-renderer": "^18.2.0"
  },
  "jest": {
    "preset": "jest-expo",
    "isolatedModules": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native(-.*)?|@react-native(-community)?|@rneui|expo(-.*)?|@expo(-.*)?/.)/)"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
  },
  "dependencies": {
    "@react-native-community/netinfo": "^9.4.1",
    "expo-file-system": "^15.6.0",
    "project-can-json": "^1.0.1"
  }
}
